<?xml version="1.0" encoding="UTF-8"?>
   <!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<!-- <constant name="struts.devMode" value="true" /> -->
	<constant name="struts.enable.DynamicMethodInvocation" value="true" />

	<package name="web" extends="struts-default">

	<action name="resume" class="com.web.ResumeAction">
		<result name="create">resume.create.jsp</result>
		<result name="list" type="redirect">resume!list.action</result>
		<result name="listPage">resume.list.jsp</result>
		<result name="view">resume.view.jsp</result>
	</action>
 
</package>
</struts>